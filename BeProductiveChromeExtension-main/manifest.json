{
    "manifest_version": 3,
    "name": "Website Blocker",
    "version": "0.1.1",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["background.js"]
      }
    ],
    "icons": {
        "48": "icons/icon48.png", 
        "128": "icons/icon128.png" },
    "action": {
      "default_popup": "Popup.html",
      "default_title": "Website Blocker"
    },
    "permissions": [
      "storage",  
      "activeTab",  
      "webNavigation",   
      "storage",  
      "tabs" 
    ]
  }